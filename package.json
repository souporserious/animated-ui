{
  "name": "animated-ui",
  "version": "0.2.0",
  "description": "Animated UI components.",
  "main": "lib/index.js",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "build:lib": "babel src --out-dir lib",
    "build": "npm run build:lib && NODE_ENV=production webpack --config webpack.prod.config.js",
    "dev": "webpack-dev-server --devtool eval --hot --progress --colors --host 0.0.0.0",
    "postbuild": "NODE_ENV=production TARGET=minify webpack --config webpack.prod.config.js",
    "prebuild": "rm -rf dist && mkdir dist",
    "prepublish": "npm run build",
    "test": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "babel": {
    "presets": ["es2015", "stage-0", "react", "react-native"]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/souporserious/react-fluid-container"
  },
  "keywords": [
    "react",
    "reactjs",
    "react-component",
    "animation",
    "ui",
    "collapse",
    "fluid",
    "toggle"
  ],
  "author": "Travis Arnold <travis@souporserious.com> (http://souporserious.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/souporserious/animated-ui/issues"
  },
  "homepage": "https://github.com/souporserious/animated-ui",
  "peerDependencies": {
    "prop-types": "^15.5.0",
    "react": "^15.5.0",
    "react-dom": "^15.5.0"
  },
  "dependencies": {
    "animated": "^0.2.0",
    "resize-observer-polyfill": "^1.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-native": "^2.0.0",
    "babel-preset-stage-0": "^6.16.0",
    "chokidar": "^1.6.1",
    "create-styled-element": "^0.4.0",
    "css-loader": "^0.25.0",
    "enzyme": "^2.9.1",
    "glamor": "next",
    "http-server": "^0.9.0",
    "jest": "^20.0.4",
    "node-libs-browser": "^1.0.0",
    "node-sass": "^4.5.3",
    "polished": "^1.2.1",
    "postcss-loader": "^2.0.6",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-aria": "^0.4.0",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1"
  }
